{
  "name": "thoughtmarks-landing",
  "type": "module",
  "version": "2.0.1",
  "description": "Thoughtmarks marketing landing page - Astro + React",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "eslint \"src/**/*.{ts,tsx}\" \"worker/**/*.ts\"",
    "lint:guard": "eslint \"src/**/*.{ts,tsx}\" \"worker/**/*.ts\" --max-warnings=0",
    "build:legacy": "node scripts/build/assemble.mjs",
    "deploy": "astro build && wrangler pages deploy dist --project-name thoughtmarks-landing",
    "deploy:production": "astro build && wrangler pages deploy dist --project-name thoughtmarks-landing --branch production",
    "worker:deploy": "cd worker && wrangler deploy",
    "worker:dev": "cd worker && wrangler dev",
    "prepare": "husky"
  },
  "dependencies": {
    "@astrojs/react": "^4.2.1",
    "@astrojs/tailwind": "^6.0.2",
    "astro": "^5.8.0",
    "framer-motion": "^12.12.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241106.0",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "postcss": "^8.5.4",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3",
    "wrangler": "^3.0.0"
  },
  "packageManager": "pnpm@9.15.0"
  }
